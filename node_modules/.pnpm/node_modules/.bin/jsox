#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/koranit/attendance/node_modules/.pnpm/jsox@1.2.125/node_modules/jsox/lib/node_modules:/home/koranit/attendance/node_modules/.pnpm/jsox@1.2.125/node_modules/jsox/node_modules:/home/koranit/attendance/node_modules/.pnpm/jsox@1.2.125/node_modules:/home/koranit/attendance/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/koranit/attendance/node_modules/.pnpm/jsox@1.2.125/node_modules/jsox/lib/node_modules:/home/koranit/attendance/node_modules/.pnpm/jsox@1.2.125/node_modules/jsox/node_modules:/home/koranit/attendance/node_modules/.pnpm/jsox@1.2.125/node_modules:/home/koranit/attendance/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jsox/lib/cli.js" "$@"
else
  exec node  "$basedir/../jsox/lib/cli.js" "$@"
fi
